<h1>Games#new</h1>
<h3><%= @letters %></h3>

<form action="/score" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="text" name="word" placeholder="type your word">
  <input type="submit">
  <%= hidden_field_tag :grid, @letters %>
  <%= hidden_field_tag :start_time, Time.now %>

</form>

